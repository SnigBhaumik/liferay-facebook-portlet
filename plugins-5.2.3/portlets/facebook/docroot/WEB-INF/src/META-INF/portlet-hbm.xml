<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping default-lazy="false" auto-import="false">
	<class name="org.infoaxon.fkm.facebook.model.impl.FacebookImpl" table="fkm_facebook">
		<cache usage="read-write" />
		<id name="userid" type="long">
			<generator class="assigned" />
		</id>
		<property name="accesstoken" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="code" column="code_" type="com.liferay.portal.dao.orm.hibernate.StringType" />
	</class>
</hibernate-mapping>